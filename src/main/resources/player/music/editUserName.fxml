<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/21.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="player.music.EditUsernameController">
    <VBox alignment="CENTER" prefHeight="144.0" prefWidth="249.0" spacing="10" style="-fx-background-color: #2d3144;" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
        <Label text="Edit Username" textFill="#f0ecec">
         <font>
            <Font name="JetBrains Mono NL SemiBold" size="13.0" />
         </font></Label>
        <TextField fx:id="newUsernameField" prefHeight="14.0" prefWidth="220.0">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin></TextField>
        <Button onAction="#applyUsername" style="-fx-background-color: #a9dc76;" text="Yes">
         <font>
            <Font name="JetBrains Mono Regular" size="13.0" />
         </font></Button>
        <Button onAction="#cancel" style="-fx-background-color: #ff6188;" text="No">
         <font>
            <Font name="JetBrains Mono Regular" size="13.0" />
         </font></Button>
    </VBox>
</AnchorPane>
